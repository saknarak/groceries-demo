<Page loaded="pageLoad">
  <StackLayout>
    <Label text="{{ next30day }}"/>
    <ListView items="{{ groceryList }}">
      <ListView.itemTemplate>
        <StackLayout orientation="horizontal">
          <Label class="name" text="{{ name }}"/>
          <Label class="qty" text="{{ qty }}"/>
        </StackLayout>      
      </ListView.itemTemplate>
    </ListView>
  </StackLayout>
</Page>